<?xml version="1.0"?>
	<robot name="nox">
		<link name="base_link">
		</link>

		<link name="laser">
		</link>

		<link name="imu_link">
		</link>

		<joint name="base_link_to_laser" type="fixed">
			<parent link="base_link"/>
			<child link="laser"/>
			<origin xyz="0.0 0.0 0.15" rpy="0.0 0.0 0.0"/>
                        <axis xyz="0 0 0"/>
		</joint>

		<joint name="base_link_to_imu" type="fixed">
			<parent link="base_link"/>
			<child link="imu_link"/>
			<origin xyz="0.0 0.05 0.0" rpy="0.0 0.0 0.0"/>
                        <axis xyz="0 0 0"/>
		</joint>
		
		
	</robot>
